{
    "name": "waldhacker/pseudify-ai",
    "description": "pseudify AI - the database pseudonymizer",
    "license": [
        "GPL-2.0-or-later"
    ],
    "type": "library",
    "authors": [
        {
            "name": "Ralf Zimmermann",
            "email": "hello@waldhacker.dev",
            "homepage": "https://waldhacker.dev",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/waldhacker/pseudify-ai/",
    "support": {
        "issues": "https://github.com/waldhacker/pseudify-ai/issues"
    },
    "require": {
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.11",
        "egulias/email-validator": "^4.0",
        "fakerphp/faker": "^1.23",
        "knplabs/knp-menu-bundle": "^3.4",
        "knplabs/knp-paginator-bundle": "^6.6",
        "phpdocumentor/reflection-docblock": "^5.6",
        "symfony/asset": "^6.4",
        "symfony/asset-mapper": "^6.4",
        "symfony/console": "^6.4",
        "symfony/debug-bundle": "^6.4",
        "symfony/dotenv": "^6.4",
        "symfony/expression-language": "^6.4",
        "symfony/filesystem": "^6.4",
        "symfony/finder": "^6.4",
        "symfony/flex": "^2.4",
        "symfony/form": "^6.4",
        "symfony/framework-bundle": "^6.4",
        "symfony/http-foundation": "^6.4",
        "symfony/monolog-bundle": "^3.10",
        "symfony/process": "^6.4",
        "symfony/property-access": "^6.4",
        "symfony/property-info": "^6.4",
        "symfony/runtime": "^6.4",
        "symfony/security-csrf": "^6.4",
        "symfony/serializer": "^6.4",
        "symfony/string": "^6.4",
        "symfony/translation": "^6.4",
        "symfony/twig-bundle": "^6.4",
        "symfony/uid": "^6.4",
        "symfony/validator": "^6.4",
        "symfony/var-dumper": "^6.4",
        "symfony/yaml": "^6.4",
        "theodo-group/llphant": "^0.9.1",
        "twig/extra-bundle": "^3.8",
        "twig/intl-extra": "^3.8",
        "twig/twig": "^3.8"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.0",
        "ergebnis/composer-normalize": "^2.41",
        "friendsofphp/php-cs-fixer": "^3.48",
        "rector/rector": "^1.2",
        "roave/security-advisories": "dev-latest",
        "symfony/maker-bundle": "^1.52",
        "symfony/phpunit-bridge": "^6.4",
        "vimeo/psalm": "^5.20"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Waldhacker\\Pseudify\\": [
                "../userdata/src/"
            ],
            "Waldhacker\\Pseudify\\Core\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waldhacker\\Pseudify\\Core\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/pseudify"
    ],
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "composer-normalize": {
            "indent-size": 4,
            "indent-style": "space"
        },
        "symfony": {
            "docker": false
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "cgl:check": [
            "php-cs-fixer --verbose --dry-run fix"
        ],
        "cgl:fix": [
            "php-cs-fixer --verbose fix"
        ],
        "composer:normalize:check": [
            "@composer normalize --dry-run"
        ],
        "composer:normalize:fix": [
            "@composer normalize"
        ],
        "psalm": [
            "psalm"
        ],
        "psalm:gen-baseline": [
            "psalm --set-baseline=psalm-baseline.xml"
        ],
        "rector": [
            "rector process src/"
        ],
        "stan": [
            "phpstan analyze"
        ],
        "stan:gen-baseline": [
            "phpstan analyze --generate-baseline phpstan.baseline.neon"
        ]
    }
}
